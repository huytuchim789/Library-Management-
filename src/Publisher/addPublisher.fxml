                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </JFXTextField>
            <JFXTextField id="publisher" fx:id="phone" layoutX="10.0" layoutY="110.0" opacity="0.71" promptText="Phone">
